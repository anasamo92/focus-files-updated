{
    "name": "focus-files-updated",
    "version": "1.0.7",
    "description": "Focus file add on.",
    "main": "index.js",
    "scripts": {
        "build": "better-npm-run babelify",
        "fix-lint": "eslint src/** --ext .js,.jsx --fix --ignore-pattern __tests__ --ignore-pattern example",
        "test": "echo \"No test specified\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anasamo92/focus-files-updated.git"
    },
    "betterScripts": {
        "babelify": {
            "command": "babel src/ --out-dir . && (test -d \"component/style/\" || mkdir \"component/style/\") &&  cp -r ./src/component/style/index.scss ./component/style/index.scss",
            "env": {
                "BABEL_ENV": "production",
                "LEGACY_EXPORTS": "true"
            }
        }
    },
    "keywords": [
        "react",
        "reactjs",
        "boilerplate",
        "hot",
        "reload",
        "hmr",
        "live",
        "edit",
        "webpack"
    ],
    "babel": {
        "presets": [
            "updated"
        ]
    },
    "license": "MIT",
    "peerDependencies": {
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "prop-types": "^15.6.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-updated": "^1.0.2",
        "better-npm-run": "^0.1.1",
        "eslint-config-updated": "^1.0.2",
        "focus-core-updated": "^1.0.2",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "dependencies": {
        "dropzone": "^5.7.6",
        "uuid": "^8.3.2"
    }
}
